@model List<ResultFooterAdressDto>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
<!-- Include SweetAlert library -->
<script src="https://cdn.jsdelivr.net/npm/sweetalert2@10"></script>
<script>
	$(document).ready(function () {
		$('#btn1').click(function () {
			Swal.fire({
				title: "Şartlar & Koşullar",
				text: "Ofislerimizde yer alan anlaşma dökümanlarına ulaşabilirsiniz. Bütün Süreç, Hasar Ceza ve Geçişler ilgili detaylar yer almaktadır. Keyifli yolculuklar dileriz.",
				icon: "success"
			});
		});
		$('#btn2').click(function () {
			Swal.fire({
				title: "Güvenlik",
				text: "Carbook Web Sitemiz AsspNet Core teknolojisinde Sha256 şifreleme tekniğiyle verilerin tamamen şirteli olarak tutulması alt yapısına sahiptir. Sistemimizde JWT doğrulaması yapılmaktadır. Kullancılarımızın ödeme yaptıkları kart bilgileri sistemde kesinlikle saklanmamaktadır.",
				icon: "security"
			});
		});
		$('#btn3').click(function () {
			Swal.fire({
				title: "Çerez Politikası",
				text: "Sizlere daha iyi hizmet vermek adına tarayıcı çerezlerini kullanmaktayız.",
				icon: "security"
			});
		});
		$('#btn4').click(function () {
			Swal.fire({
				title: "SSS",
				text: "",
				icon: "success"
			});
		});
		$('#btn5').click(function () {
			Swal.fire({
				title: "Ödeme Yöntemleri",
				text: "Sizlere daha iyi hizmet vermek adına tarayıcı çerezlerini kullanmaktayız.",
				icon: "success"
			});
		});
		$('#btn6').click(function () {
			Swal.fire({
				title: "Nasıl Çalışır?",
				text: "",
				icon: "success"
			});
		});
	});
</script>
@foreach (var item in Model)
{


	<footer class="ftco-footer ftco-bg-dark ftco-section">
		<div class="container">
			<div class="row mb-5">
				<div class="col-md">
					<div class="ftco-footer-widget mb-4">
						<h2 class="ftco-heading-2"><a href="#" class="logo">Car<span>book</span></a></h2>
						<p>@item.Description</p>
						<ul class="ftco-footer-social list-unstyled float-md-left float-lft mt-5">
							<li class="ftco-animate"><a href="#"><span class="icon-twitter"></span></a></li>
							<li class="ftco-animate"><a href="#"><span class="icon-facebook"></span></a></li>
							<li class="ftco-animate"><a href="#"><span class="icon-instagram"></span></a></li>
						</ul>
					</div>
				</div>
				<div class="col-md">
					<div class="ftco-footer-widget mb-4 ml-md-5">
						<h2 class="ftco-heading-2">Hızlı Erişim</h2>
						<ul class="list-unstyled">
							<li><a href="/About/Index" class="py-2 d-block">Hakkımızda</a></li>
							<li><a href="/Car/Index" class="py-2 d-block">Hizmetlerimiz</a></li>
							<li><a  id="btn1" class="py-2 d-block">Şarlar & Koşullar</a></li>

							<li><a href="#" id="btn2" class="py-2 d-block">Güvenlik</a></li>
							<li><a href="#" id="btn3"  class="py-2 d-block">Çerez Politikası</a></li>
						</ul>
					</div>
				</div>
				<div class="col-md">
					<div class="ftco-footer-widget mb-4">
						<h2 class="ftco-heading-2">Müşteri Desteği</h2>
						<ul class="list-unstyled">
							<li><a href="#" id="btn4" class="py-2 d-block">Sıkça Sorulanlar</a></li>
							<li><a href="#" id="btn5" class="py-2 d-block">Ödeme Yöntemleri</a></li>
							<li><a href="/CarPricing/Index" class="py-2 d-block">Kiralama</a></li>
							<li><a href="#" id="btn6" class="py-2 d-block">Nasıl Çalışır?</a></li>
							<li><a href="/Contact/Index" class="py-2 d-block">Bize yazın</a></li>
						</ul>
					</div>
				</div>
				<div class="col-md">
					<div class="ftco-footer-widget mb-4">
						<h2 class="ftco-heading-2">Bir sorunuz mu var?</h2>
						<div class="block-23 mb-3">
							<ul>
								<li><span class="icon icon-map-marker"></span><span class="text">@item.Adress</span></li>
								<li><a href="#"><span class="icon icon-phone"></span><span class="text">@item.Phone</span></a></li>
								<li><a href="#"><span class="icon icon-envelope"></span><span class="text">@item.Email</span></a></li>
							</ul>
						</div>
					</div>
				</div>
			</div>
			<div class="row">
				<div class="col-md-12 text-center">

					<p>
						<!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
						Copyright &copy;<script>document.write(new Date().getFullYear());</script> All rights reserved | This template is made with <i class="icon-heart color-danger" aria-hidden="true"></i> by <a href="https://colorlib.com" target="_blank">Colorlib</a>
						<!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
					</p>
				</div>
			</div>
		</div>
	</footer>
	<!-- loader -->
	<div id="ftco-loader" class="show fullscreen"><svg class="circular" width="48px" height="48px"><circle class="path-bg" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke="#eeeeee" /><circle class="path" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke-miterlimit="10" stroke="#F96D00" /></svg></div>
}

